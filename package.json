{
  "name": "generatepress-child-tailwind",
  "version": "1.0.0",
  "description": "Tailwind CSS build for GeneratePress Child Theme",
  "main": "index.js",
  "scripts": {
    "build": "tailwindcss -i ./src/input.css -o ./assets/css/style.min.css --minify",
    "build:dev": "tailwindcss -i ./src/input.css -o ./assets/css/style.css",
    "watch": "tailwindcss -i ./src/input.css -o ./assets/css/style.css --watch",
    "dev": "npm run watch"
  },
  "keywords": [
    "wordpress",
    "tailwind",
    "generatepress"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  }
}
